{
  "name": "encounter-svc",
  "version": "1.0.0",
  "description": "encounter service server",
  "main": "graphql/index.js",
  "author": "Mostafa Sholkamy",
  "license": "MIT",
  "scripts": {
    "start": "node graphql",
    "deploy": "cd prisma && env-cmd ../.env prisma deploy",
    "dev": "env-cmd ./.env nodemon --preserve-symlinks -e js,graphql graphql/index.js",
    "deploy-test": "cd prisma && env-cmd ../.env.test prisma deploy",
    "reset-test": "cd prisma && prisma reset -e ../.env.test -f",
    "lint": "standard",
    "jest": "env-cmd ./.env.test jest",
    "test": "run-s reset-test jest",
    "delete": "cd prisma && env-cmd ../.env prisma delete"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dev": "^0.1.3",
    "graphql-import": "^0.7.1",
    "graphql-yoga": "^1.17.4",
    "just-date": "^3.1.3",
    "prisma-binding": "^2.3.3"
  },
  "devDependencies": {
    "env-cmd": "^8.0.2",
    "graphql-request": "^1.8.2",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prisma": "^1.27.0",
    "standard": "^12.0.1"
  },
  "resolutions": {
    "graphql": "14.2.1"
  },
  "standard": {
    "ignore": [
      "playground/static",
      "node_modules"
    ],
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  }
}
